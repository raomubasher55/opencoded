# LLM Service Configuration
PORT=4002
NODE_ENV=development
JWT_SECRET=your_jwt_secret_here

# Default LLM Provider
DEFAULT_LLM_PROVIDER=openai # Options: openai, anthropic, google, bedrock
DEFAULT_LLM_MODEL=gpt-4
DEFAULT_LLM_API_KEY=your_api_key_here

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_DEFAULT_MODEL=gpt-4
OPENAI_ORGANIZATION_ID=your_org_id_if_applicable

# Anthropic Configuration
ANTHROPIC_API_KEY=your_anthropic_api_key_here
ANTHROPIC_DEFAULT_MODEL=claude-3-opus-20240229

# Google Configuration
GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_DEFAULT_MODEL=gemini-pro

# AWS Bedrock Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
BEDROCK_DEFAULT_MODEL=anthropic.claude-v2

# Logging
LOG_LEVEL=info